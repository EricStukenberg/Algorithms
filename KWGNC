const kidsWithCandies = function(candies, extraCandies) {
    const max = Math.max(...candies)
    const min = max - extraCandies
    const len = candies.length
    const res = []
    for(let i = 0; i < len; i++) {
        if(candies[i] < min) {
            res << false
        } else {
            res << true
        }
    }
    return res;
};
